{
  "name": "spryker/merchant-product-gui",
  "type": "library",
  "description": "MerchantProductGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/availability-gui-extension": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/merchant": "^3.0.0",
    "spryker/merchant-product": "^0.1.1",
    "spryker/product-management-extension": "^1.4.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel-orm": "*"
  },
  "suggest": {
    "spryker/propel-orm": "If you want to use transaction functionality, minimum required version: 1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
